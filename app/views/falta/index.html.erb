<p id="notice"><%= notice %></p>

<h1>Falta</h1>

<table>
  <thead>
    <tr>
      <th>Presenças</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @falta.each do |faltum| %>
      <tr>
        <td><%= faltum.presenças %></td>
        <td><%= link_to 'Show', faltum %></td>
        <td><%= link_to 'Edit', edit_faltum_path(faltum) %></td>
        <td><%= link_to 'Destroy', faltum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Faltum', new_faltum_path %>
